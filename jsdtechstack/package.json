{
  "name": "chatgpt-sidebar",
  "displayName": "ChatGPT Sidebar",
  "description": "A VS Code extension that integrates ChatGPT into the activity sidebar.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:chatgptView"
  ],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "chatgpt",
          "title": "ChatGPT",
          "icon": "images/chatgpt.svg"
        }
      ]
    },
    "views": {
      "chatgpt": [
        {
          "id": "chatgptView",
          "name": "ChatGPT Chat"
        }
      ]
    }
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
